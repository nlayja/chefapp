<p id="notice"><%= notice %></p>

<h1>Invitados</h1>

<table>
  <thead>
    <tr>
      <th>Chef</th>
      <th>Name</th>
      <th>Class</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @invitados.each do |invitado| %>
      <tr>
        <td><%= invitado.chef_id %></td>
        <td><%= invitado.name %></td>
        <td><%= invitado.class %></td>
        <td><%= invitado.email %></td>
        <td><%= link_to 'Show', invitado %></td>
        <td><%= link_to 'Edit', edit_invitado_path(invitado) %></td>
        <td><%= link_to 'Destroy', invitado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Invitado', new_invitado_path %>
